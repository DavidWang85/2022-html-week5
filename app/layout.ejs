            <a class="menu-link text-dark d-flex align-middle fw-bold" href="#">
              <span class="material-icons me-3">person</span>Admin
            </a>
          </li>
          <li class="menu-ite mb-7">
            <a class="menu-link text-dark d-flex align-middle fw-bold" href="#">
              <span class="material-icons me-3">settings</span>settings
            </a>
          </li>
        </ul>
      </div>
      <div class="user d-flex">
        <img class="user-img me-2" src="https://raw.githubusercontent.com/hexschool/2022-web-layout-training/main/week5/andychen.jpeg" alt="">
        <div class="user-info">
          <p class="text-dark fw-bold line-light-md mb-2">Andy Chen</p>
          <p class="text-secondary font-size-sm line-light-md">andy0401@mail.com</p>
        </div>
      </div>
    </section>
    <section class="w-77">
      <%- contents %>
    </section>
  </div>
  <script src="./assets/js/vendors.js"></script>
  <script src="https://cdn.jsdelivr.net/npm/@popperjs/core@2.10.1/dist/umd/popper.min.js"
    integrity="sha384-W8fXfP3gkOKtndU4JGtKDvXbO53Wy8SZCQHczT5FMiiqmQfUpWbYdTil/SxwZgAN" crossorigin="anonymous"></script>
  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.1/dist/js/bootstrap.min.js"
