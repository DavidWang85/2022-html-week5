<body>
  <div class="d-flex">
    <section class="slidebar d-flex flex-column justify-content-between bg-light w-23">
      <div>
        <h1><a class="logo" href="#">Logo</a></h1>
        <ul class="menu">
          <li class="menu-item mb-7">
            <a class="menu-link text-dark d-flex align-middle fw-bold" href="#">
              <span class="material-icons me-3">assessment</span>Dashboard
            </a>
          </li>
          <li class="menu-item mb-7">
            <a class="menu-link text-dark d-flex align-middle fw-bold" href="#">
              <span class="material-icons me-3">question_answer</span>Question
            </a>
          </li>
          <li class="menu-ite mb-7">
            <a class="menu-link text-dark d-flex align-middle fw-bold" href="#">
              <span class="material-icons me-3">assignment</span>Assignment
            </a>
          </li>
          <li class="menu-ite mb-7">
            <a class="menu-link text-dark d-flex align-middle fw-bold" href="#">
              <span class="material-icons me-3">video_library</span>Course
